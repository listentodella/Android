class LogDecelerateInterpolator
!!!462594.java!!!	LogDecelerateInterpolator(in base : int, in drift : int)
        mBase = base;
        mDrift = drift;

        mLogScale = 1f / computeLog(1, mBase, mDrift);
!!!462722.java!!!	computeLog(in t : float, in base : int, in drift : int) : float
        return (float) -Math.pow(base, -t) + 1 + (drift * t);
!!!462850.java!!!	getInterpolation(in t : float) : float
        return computeLog(t, mBase, mDrift) * mLogScale;
